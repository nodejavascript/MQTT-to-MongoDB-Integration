{
  "name": "mqtt-to-mongodb-integration",
  "version": "1.0.0",
  "description": "This repository contains an application that subscribes to all topics on an MQTT broker and saves the received messages into a MongoDB database. It uses environment variables for sensitive credentials.",
  "scripts": {
    "start": "TZ='UTC' node dist/app.js",
    "dev": "TZ='UTC' nodemon src/app.ts",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "build": "tsc",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "author": "nodejavascript.com'",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nodejavascript/MQTT-to-MongoDB-Integration.git"
  },
  "bugs": {
    "url": "https://github.com/nodejavascript/MQTT-to-MongoDB-Integration/issues"
  },
  "homepage": "https://github.com/nodejavascript/MQTT-to-MongoDB-Integration#readme",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "mongodb": "6.3.0",
    "mongoose": "8.0.3",
    "mqtt": "5.3.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/node": "^20.8.9",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "mongodb-memory-server": "^9.1.3"
  }
}
